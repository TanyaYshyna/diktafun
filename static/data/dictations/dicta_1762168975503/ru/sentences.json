    },
    {
      "key": "018",
      "speaker": "1",
      "text": "двадцать, девушка, покупать",
      "audio": "018_ru_avto.mp3",
      "audio_avto": "018_ru_avto.mp3",
      "audio_user": "",
      "audio_mic": "",
      "start": 0,
      "end": 0,
      "chain": false,
      "explanation": "Tj читается как «чь»."
    },
    {
      "key": "019",
      "speaker": "1",
      "text": "ребёнок, хорошо, книга",
      "audio": "019_ru_avto.mp3",
      "audio_avto": "019_ru_avto.mp3",
      "audio_user": "",
      "audio_mic": "",
      "start": 0,
      "end": 0,
